<templateSet group="Custom">
  <template name="ctext" value="{{ forms.textField({&#10;    label: '$LABEL$',&#10;    name: '$NAME$',&#10;    value: settings.$NAME$,&#10;}) }}" description="A Craft textfield" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="capitalize(spaceSeparated(NAME))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="clightswitch" value="{{ forms.lightswitchField({&#10;    label: '$LABEL$',&#10;    name: '$NAME$',&#10;    on: settings.$NAME$,&#10;}) }}" description="A Craft Lighswitch field" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="capitalize(spaceSeparated(NAME))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="cforms" value="{% import '_includes/forms' as forms %}" description="Include Craft Forms" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="cdate" value="{{ forms.dateField({&#10;    label: '$LABEL$',&#10;    name: '$NAME$',&#10;    value: settings.$NAME$,&#10;}) }}" description="A Craft datefield" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="capitalize(spaceSeparated(NAME))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="ccsrf" value="{{ csrfInput() }}" description="A craft CSRF form token" toReformat="true" toShortenFQNames="true">
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="chidden" value="{{ forms.hidden({&#10;    name: '$NAME$',&#10;    value: '$VALUE$'&#10;}) }}" description="A craft hidden field" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="ccolor" value="{{ forms.colorField({&#10;    label: '$LABEL$',&#10;    name: '$NAME$',&#10;    value: settings.$NAME$,&#10;}) }}" description="A Craft textfield" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="capitalize(spaceSeparated(NAME))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="ccheckbox" value="{{ forms.checkbox({&#10;    name: '$NAME$',&#10;    value: '$NAME$',&#10;}) }}" description="A Craft checkbox" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="cfile" value="{{ forms.fileField({&#10;    label: '$LABEL$',&#10;    name: '$NAME$',&#10;    class: '$NAME$',&#10;}) }}" description="A Craft file field" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="capitalize(spaceSeparated(NAME))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="cselect" value="{{ forms.selectField({&#10;    label: '$LABEL$',&#10;    name: '$NAME$',&#10;    value: '$NAME$',&#10;    options: settings.$NAME$,&#10;}) }}" description="A Craft selectfield" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="capitalize(spaceSeparated(NAME))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="ctextarea" value="{{ forms.textareaField({&#10;    label: '$LABEL$',&#10;    name: '$NAME$',&#10;    value: settings.$NAME$,&#10;    rows: 10&#10;}) }}" description="A Craft textarea field" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="LABEL" expression="capitalize(spaceSeparated(NAME))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="ctime" value="{{ forms.timeField({&#10;    label: '$LABEL$',&#10;    name: '$NAME$',&#10;    value: settings.$NAME$,&#10;    minuteIncrement: $INCREMENT$&#10;}) }}" description="A Craft timefield" toReformat="true" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="INCREMENT" expression="" defaultValue="30" alwaysStopAt="true" />
    <variable name="LABEL" expression="capitalize(spaceSeparated(NAME))" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="HTML_TEXT" value="true" />
      <option name="Twig" value="true" />
    </context>
  </template>
  <template name="cment" value="// --------------------------------------------------------------------------------------------------&#10;// $TITLE$&#10;// --------------------------------------------------------------------------------------------------" description="Create Comment" toReformat="true" toShortenFQNames="true">
    <variable name="TITLE" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="CSS_RULESET_LIST" value="true" />
    </context>
  </template>
</templateSet>